<h1>Update Artist</h1>

<%= form_for @artist do |f| %>
   <%= f.label "Name:" %>
   <%= f.text_field :name %> <br>
   <%= f.label "Age:" %>
   <%= f.text_field :age %> <br>
   <%= f.label "Title:" %>
   <%= f.text_field :title %> <br>

   <%= f.label "Instrument:" %>
   <%= f.collection_select :instrument.id, Instrument.all, :id, :name %>

   <%= f.submit %> 
<% end %>